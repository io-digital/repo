{
    "name": "io-digital/repo",
    "type": "library",
    "description": "Scaffold repository pattern in Laravel",
    "keywords": [
        "repo",
        "laravel",
        "repository",
        "pattern"
    ],
    "homepage": "https://github.com/io-digital/repo",
    "license": "MIT",
    "authors": [
        {
            "name": "Gareth Nicholson",
            "email": "gareth.nic@io.co.za",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "IoDigital\\Repo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IoDigital\\Repo\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
